<%= form_with model: @trainer, local: true do |f|  %>
  <%= f.label :name %>
  <%= f.text_field :name %><br/>
  <%= f.label :certified %>
  <%= f.check_box :certified, {}, true, false %><br/>
  <%= f.label :years_of_experience %>
  <%= f.number_field :years_of_experience, in: 0..150 %><br/>
  <%= f.submit 'Create Trainer' %>
<% end %>
